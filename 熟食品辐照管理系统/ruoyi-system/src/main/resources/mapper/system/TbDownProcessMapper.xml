<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ruoyi.system.mapper.TbDownProcessMapper">
    
    <resultMap type="TbDownProcess" id="TbDownProcessResult">
        <result property="id"    column="id"    />
        <result property="radiNo"    column="radi_no"    />
        <result property="goodsName"    column="goods_name"    />
        <result property="goodsNo"    column="goods_no"    />
        <result property="goodsDime"    column="goods_dime"    />
        <result property="area"    column="area"    />
        <result property="downMan"    column="down_man"    />
        <result property="startDate"    column="start_date"    />
        <result property="endDate"    column="end_date"    />
        <result property="note1"    column="note1"    />
        <result property="note2"    column="note2"    />
        <result property="note3"    column="note3"    />
    </resultMap>

    <sql id="selectTbDownProcessVo">
        select id, radi_no, goods_name, goods_no, goods_dime, area, down_man, start_date, end_date, note1, note2, note3 from tb_down_process
    </sql>

    <select id="selectTbDownProcessList" parameterType="TbDownProcess" resultMap="TbDownProcessResult">
        <include refid="selectTbDownProcessVo"/>
        <where>  
            <if test="radiNo != null  and radiNo != ''"> and radi_no = #{radiNo}</if>
            <if test="downMan != null  and downMan != ''"> and down_man = #{downMan}</if>
        </where>
    </select>
    
    <select id="selectTbDownProcessById" parameterType="Long" resultMap="TbDownProcessResult">
        <include refid="selectTbDownProcessVo"/>
        where id = #{id}
    </select>
        
    <insert id="insertTbDownProcess" parameterType="TbDownProcess" useGeneratedKeys="true" keyProperty="id">
        insert into tb_down_process
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="radiNo != null and radiNo != ''">radi_no,</if>
            <if test="goodsName != null and goodsName != ''">goods_name,</if>
            <if test="goodsNo != null and goodsNo != ''">goods_no,</if>
            <if test="goodsDime != null and goodsDime != ''">goods_dime,</if>
            <if test="area != null and area != ''">area,</if>
            <if test="downMan != null and downMan != ''">down_man,</if>
            <if test="startDate != null">start_date,</if>
            <if test="endDate != null">end_date,</if>
            <if test="note1 != null">note1,</if>
            <if test="note2 != null">note2,</if>
            <if test="note3 != null">note3,</if>
         </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="radiNo != null and radiNo != ''">#{radiNo},</if>
            <if test="goodsName != null and goodsName != ''">#{goodsName},</if>
            <if test="goodsNo != null and goodsNo != ''">#{goodsNo},</if>
            <if test="goodsDime != null and goodsDime != ''">#{goodsDime},</if>
            <if test="area != null and area != ''">#{area},</if>
            <if test="downMan != null and downMan != ''">#{downMan},</if>
            <if test="startDate != null">#{startDate},</if>
            <if test="endDate != null">#{endDate},</if>
            <if test="note1 != null">#{note1},</if>
            <if test="note2 != null">#{note2},</if>
            <if test="note3 != null">#{note3},</if>
         </trim>
    </insert>

    <update id="updateTbDownProcess" parameterType="TbDownProcess">
        update tb_down_process
        <trim prefix="SET" suffixOverrides=",">
            <if test="radiNo != null and radiNo != ''">radi_no = #{radiNo},</if>
            <if test="goodsName != null and goodsName != ''">goods_name = #{goodsName},</if>
            <if test="goodsNo != null and goodsNo != ''">goods_no = #{goodsNo},</if>
            <if test="goodsDime != null and goodsDime != ''">goods_dime = #{goodsDime},</if>
            <if test="area != null and area != ''">area = #{area},</if>
            <if test="downMan != null and downMan != ''">down_man = #{downMan},</if>
            <if test="startDate != null">start_date = #{startDate},</if>
            <if test="endDate != null">end_date = #{endDate},</if>
            <if test="note1 != null">note1 = #{note1},</if>
            <if test="note2 != null">note2 = #{note2},</if>
            <if test="note3 != null">note3 = #{note3},</if>
        </trim>
        where id = #{id}
    </update>

    <delete id="deleteTbDownProcessById" parameterType="Long">
        delete from tb_down_process where id = #{id}
    </delete>

    <delete id="deleteTbDownProcessByIds" parameterType="String">
        delete from tb_down_process where id in 
        <foreach item="id" collection="array" open="(" separator="," close=")">
            #{id}
        </foreach>
    </delete>

</mapper>